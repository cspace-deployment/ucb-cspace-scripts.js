{
  "name": "ucb-cspace-scripts",
  "version": "0.0.1",
  "description": "UC Berkeley CollectionSpace scripts",
  "author": "Ray Lee <rhlee@berkeley.edu>",
  "license": "ECL-2.0",
  "keywords": [
    "collectionspace",
    "scripts",
    "uc",
    "berkeley"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cspace-deployment/ucb-cspace-scripts.js.git"
  },
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "cross-env BABEL_ENV=cjs babel ./src -d lib",
    "check": "npm run lint && npm run test && npm run coverage",
    "clean": "rimraf lib coverage",
    "coverage": "istanbul report --root coverage text && istanbul report --root coverage lcov",
    "lint": "eslint *.js src test",
    "prepare": "npm run clean && npm run build",
    "test": "cross-env NODE_ENV=test nyc mocha --compilers js:babel-register --recursive test"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-istanbul": "^1.0.3",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^2.13.3",
    "cross-env": "^5.1.3",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.8.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.3",
    "nyc": "^11.5.0",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "cspace-api": "^0.0.13",
    "cspace-input": "^0.12.48",
    "csv-parse": "^2.0.4",
    "csv-stringify": "^2.0.4",
    "form-data": "^2.3.2",
    "lodash": "^4.17.5",
    "stream-transform": "^1.0.2",
    "xml2js": "^0.4.19"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "json"
    ],
    "report-dir": "./coverage/Node.js",
    "sourceMap": false,
    "instrument": false,
    "cache": true
  }
}
